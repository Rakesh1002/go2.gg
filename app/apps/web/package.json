    "build:cf": "npx @opennextjs/cloudflare build",
    "start": "next start",
    "lint": "biome lint ./app ./components ./lib ./hooks",
    "lint:fix": "biome lint --fix ./app ./components ./lib ./hooks",
    "format": "biome format --write ./app ./components ./lib ./hooks",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf .next .turbo .open-next node_modules",
    "preview": "pnpm build && pnpm build:cf && wrangler dev --env production",
    "deploy": "pnpm build && pnpm build:cf && wrangler deploy --env production",
    "deploy:staging": "pnpm build && pnpm build:cf && wrangler deploy --env staging",
    "cf:dev": "wrangler dev",
    "cf:deploy": "wrangler deploy",
    "cf:deploy:staging": "wrangler deploy --env staging",
    "cf:deploy:production": "wrangler deploy --env production",
