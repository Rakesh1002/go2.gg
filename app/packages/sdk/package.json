{
  "name": "@go2/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript SDK for Go2 URL Shortener",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": ["go2", "url-shortener", "short-links", "sdk", "api"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rakesh1002/go2.gg"
  }
